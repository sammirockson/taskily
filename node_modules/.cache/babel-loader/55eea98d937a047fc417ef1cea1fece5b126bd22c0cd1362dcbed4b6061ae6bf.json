/components/BoardContentView.vue'\nimport SideBarView from '@/components/SideBarView.vue'\nexport default {\n    name: \"cardDetail\",\n    components: {\n        BoardContentView, SideBarView\n    }, \n    setup() {\n        var isExpanded = ref(false)\n        var isCardTapped = ref(false)\n        return { isExpanded, isCardTapped }\n    }, \n    methods: {\n        fetchedCardInfo(cardData) {\n            console.log(\"card final info data: \", cardData)\n            this.selectedCard = cardData.card\n            this.selectedList = cardData.list\n            this.isCardTapped = true \n        }, \n        handleOverlayDismissed() {\n            this.isCardTapped = false \n            this.$router.go(-1)\n        },\n    },\n    mounted() {\n        console.log(\"card loaded\")\n    }\n}\n</script>\n<style scoped>\n.card {\n  display: flex;\n  flex-direction: row;\n  background-color: var(--color-background);\n  width: 100vw;\n  height: 100vh;\n  overscroll-behavior: none;\n}\n.posContainer {\n  height: 100%;\n  width: calc(100vw - 250px);\n  background-color: var(--color-background);\n}\n    \n</style>"],"mappings":"AAUA,SAASA,GAAE,QAAS,KAAI;AACxB,OAAOC,gBAAe,MAAO,mCAAkC;AAC/D,OAAOC,WAAU,MAAO,8BAA6B;AACrD,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACRH,gBAAgB;IAAEC;EACtB,CAAC;EACDG,KAAKA,CAAA,EAAG;IACJ,IAAIC,UAAS,GAAIN,GAAG,CAAC,KAAK;IAC1B,IAAIO,YAAW,GAAIP,GAAG,CAAC,KAAK;IAC5B,OAAO;MAAEM,UAAU;MAAEC;IAAa;EACtC,CAAC;EACDC,OAAO,EAAE;IACLC,eAAeA,CAACC,QAAQ,EAAE;MACtBC,OAAO,CAACC,GAAG,CAAC,wBAAwB,EAAEF,QAAQ;MAC9C,IAAI,CAACG,YAAW,GAAIH,QAAQ,CAACI,IAAG;MAChC,IAAI,CAACC,YAAW,GAAIL,QAAQ,CAACM,IAAG;MAChC,IAAI,CAACT,YAAW,GAAI,IAAG;IAC3B,CAAC;IACDU,sBAAsBA,CAAA,EAAG;MACrB,IAAI,CAACV,YAAW,GAAI,KAAI;MACxB,IAAI,CAACW,OAAO,CAACC,EAAE,CAAC,CAAC,CAAC;IACtB;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNT,OAAO,CAACC,GAAG,CAAC,aAAa;EAC7B;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}