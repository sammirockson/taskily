>\n        <div class=\"mainBoardConentView\">\n            <div class=\"boardListsContainer\">\n                 <div class=\"listContainer\" v-for=\"(list, index) in this.board.lists\" :key=\"list.id\">\n                    <div v-if=\"list.id ==  `listPlaceholder`\" class=\"listFooterView\" @click=\"handleAddList(list, index)\">\n                        <span id=\"addIcon\" class=\"material-symbols-outlined\">add</span>\n                        <div class=\"footerTitleContainer\">\n                            <button class=\"addCardLabel\">{{ list.id ==  `listPlaceholder` ? `New List` : `New Card` }}</button>\n                        </div>\n                       </div>\n                     <div v-else class=\"listHeaderView\" :style=\"{display: list.id ==  `listPlaceholder` && list.cards.length == 0 ? 'none' : 'flex'}\">\n                         <div class=\"badgeAndTitleContainer\">\n                            <div class=\"colorBadge\"></div>\n                            <label class=\"listNameLabel\">{{ list.listName }}</label>\n                         </div>\n                         <label class=\"listNameLabel\">•••</label>\n                     </div>\n                     <div class=\"cardAndFooterContainer\">\n                        <div class=\"cardContainer\"  v-for=\"card in list.cards\" :key=\"card.id\">\n                          <div class=\"cardCell\">\n                             <img v-if=\"card.imgURL.length > 0\" src=\"@/assets/cardPhoto.png\" class=\"cardImage\">\n                             <div class=\"dueDateContainer\">\n                             <img src=\"@/assets/clock.png\" class=\"clockIcon\">\n                                 <label class=\"dueDateLabel\">25 Feb 2024</label>\n                             </div>\n                             <label class=\"cardNameLabel\">{{ card.cardName }}</label>\n                             <div class=\"progressContainer\">\n                                <v-progress-linear\n                                :location=\"false\"\n                                bg-color=\"#92aed9\"\n                                buffer-color=\"#92aed9\"\n                                color=\"#05C597\"\n                                height=\"10\"\n                                max=\"9\"\n                                min=\"0\"\n                                model-value=\"2\"\n                                rounded>\n                             </v-progress-linear>\n                             </div> \n                             <div class=\"viewMemberContainerView\">\n                                <div class=\"boardInfoView\">\n                                    <img src=\"@/assets/eyeViews.png\" class=\"boardIcon\">\n                                    <label for=\"\">4.7k</label>\n                                    <img src=\"@/assets/comments.png\" class=\"boardIcon\">\n                                    <label for=\"\">20</label>\n                                    <img src=\"@/assets/attachment.png\" class=\"boardIcon\">\n                                    <label for=\"\">99</label>\n                                </div>\n                              <div class=\"membersContainer\">\n                                <span class=\"avatar\">\n        <img  src=\"https://picsum.photos/70\">\n    </span>\n  <span class=\"avatar\">\n        <img src=\"https://picsum.photos/80\">\n    </span>\n  <span class=\"avatar\">\n        <img src=\"https://picsum.photos/90\">\n    </span>\n  <span class=\"avatar\">\n       <img src=\"https://picsum.photos/100\">\n    </span>\n    <label for=\"\">+99</label>\n                              </div>\n                             </div>\n                          </div>\n                        </div>\n                        <div v-if=\"isCreateList && list.cards.length > 0\" class=\"createListContainer\">\n                            <textarea name=\"text\" @input=\"autoGrow(index)\" placeholder=\"Give your card a name\" class=\"addListInputField\"></textarea>\n                            <!-- <input type=\"text\" placeholder=\"Give your card a name\" v-model=\"newCardName\" class=\"addListInputField\"> -->\n                            <button v-if=\"isSavingCard\" class=\"addListBtn buttonload\">\n                               <i class=\"fa fa-circle-o-notch fa-spin\"></i> Adding... \n                            </button>\n                           <button v-else class=\"addListBtn\" @click=\"handleCreateList(list, index)\">Add Card</button>\n                        </div>\n                      <div v-else v-if=\"list.cards.length > 0\" class=\"listFooterView\" @click=\"handleAddCard(list, index)\">\n                        <span id=\"addIcon\" class=\"material-symbols-outlined\">add</span>\n                        <div class=\"footerTitleContainer\">\n                            <button class=\"addCardLabel\">New Card</button>\n                        </div>\n                       </div>\n                      \n                    </div>\n                    \n                 </div>\n           </div>\n        </div>\n        <!-- <v-overlay  v-model=\"isCreateList\" class=\"align-center justify-center\" contained>\n            <CardDetailView @newCardCreated=\"handleCreateList(list, board, index)\" :list=\"this.currentList\" :board=\"this.board\" :index=\"this.currentIndex\"/>\n        </v-overlay> -->\n    </div>\n</template>\n<script>\nimport NavBar from '@/components/NavBarView.vue'\nimport CardDetailView from '@/components/CardDetailView.vue'\nimport { ref } from 'vue'\nexport default {\n    props: [\"isExpanded\"],\n    components: {\n        NavBar, CardDetailView\n    }, \n    setup() {\n        var isSideBarExpanded = ref(true)\n        var board = ref([])\n        var isCreateList = ref(false)\n        var newCardName = ref(\"\")\n        var currentIndex = ref(0)\n        var currentList = ref(Object)\n        return { isSideBarExpanded, board, isCreateList, newCardName, currentIndex, currentList }\n    },\n    methods: {\n        autoGrow(index) {\n            let element = document.getElementsByClassName(\"addListInputField\")[index]\n            element.style.height = \"5px\";\n            element.style.height = (element.scrollHeight) + \"px\";\n        },\n        handleCreateList(list, board, index) {\n            // if (list.id == \"listPlaceholder\") {\n            //     list.id = \"newList\"\n            //     list.cards = [\n            //       {id: \"cardOne\", cardName: this.newCardName, subTitle: \"Meet up to discuss early stage of the design\", description: String, imgURL: \"google.com\", progress: 0, attachments: [File]}, \n            //     ]\n            //    this.board.lists.push(\n            //     { id: \"listPlaceholder\", listName: \"Add New List\", cards: []\n            //     }\n            //    )\n            // } else {\n            //     list.cards.push(\n            //     {id: \"cardOne\", cardName: this.newCardName, subTitle: \"Meet up to discuss early stage of the design\", description: String, imgURL: \"\", progress: 0, attachments: [File]}, \n            // )\n            // }\n           \n            this.board = board\n            this.board[index] = list\n            // var myDiv = document.getElementsByClassName('cardAndFooterContainer');\n            // myDiv[index].scrollTop = myDiv[index].scrollHeight + 100;\n            // this.newCardName = \"\"\n            // this.isCreateList = false\n        },\n        handleAddCard(list, index) {\n            // this.currentList = list\n            // this.currentIndex = index\n            // console.log(\"currentList: \", this.currentList, \"index: \", index)\n            this.isCreateList = true \n            let element = document.getElementsByClassName(\"addListInputField\")[index]\n            element.focus = true\n        }\n    },\n    watch: { \n        isExpanded: function(newVal, oldVal) {\n            console.log('Prop changed isSideBarExpanwded: ', newVal)\n            this.isSideBarExpanded = newVal\n        }\n    },  \n    mounted() {\n        this.board = { id: \"board1\", lists: [\n            { id: \"listTwo\", listName: \"TASK\", cards: [\n                {id: \"cardOne\", cardName: \"[FGE TEAM] Upcoming Tasks and Bugs (Week 28, July 8 - July 12, 2024)\", subTitle: \"Meet up to discuss early stage of the design\", description: String, imgURL: \"google.com\", progress: 0, attachments: [File]}, \n            ]\n           }, \n        //    { id: \"listTwo\", listName: \"DOING\", cards: [\n        //         {id: \"cardOne\", cardName: \"Explore UI Design\", subTitle: \"Meet up to discuss early stage of the design\", description: String, imgURL: \"google.com\", progress: 0, attachments: [File]}, \n        //     ]\n        //    }, \n        //    { id: \"listTwo\", listName: \"QA\", cards: [\n        //         {id: \"cardOne\", cardName: \"Explore UI Design\", subTitle: \"Meet up to discuss early stage of the design\", description: String, imgURL: \"google.com\", progress: 0, attachments: [File]}, \n        //         {id: \"cardOne\", cardName: \"Explore UI Design\", subTitle: \"Meet up to discuss early stage of the design\", description: String, imgURL: \"google.com\", progress: 0, attachments: [File]}, \n        //         {id: \"cardOne\", cardName: \"Explore UI Design\", subTitle: \"Meet up to discuss early stage of the design\", description: String, imgURL: \"google.com\", progress: 0, attachments: [File]}\n        //     ]\n        //    }, \n        //    { id: \"listTwo\", listName: \"RELEASE\", cards: [\n        //         {id: \"cardOne\", cardName: \"Explore UI Design\", subTitle: \"Meet up to discuss early stage of the design\", description: String, imgURL: \"google.com\", progress: 0, attachments: [File]}, \n        //         {id: \"cardOne\", cardName: \"Explore UI Design\", subTitle: \"Meet up to discuss early stage of the design\", description