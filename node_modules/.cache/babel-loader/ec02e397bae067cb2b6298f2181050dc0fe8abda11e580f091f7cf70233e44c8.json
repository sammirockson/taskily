{"ast":null,"code":"import { ref } from 'vue';\nimport { BASE_URL } from '@/config';\nimport axios from 'axios';\nimport LoginView from './LoginView.vue';\nexport default {\n  components: {\n    LoginView\n  },\n  setup() {\n    var inviter = ref(null);\n    var board = ref(null);\n    var isAuth = ref(true);\n    return {\n      inviter,\n      board,\n      isAuth\n    };\n  },\n  mounted() {\n    let routeParams = this.$route.params;\n    console.log(\"invitation params: \", routeParams);\n    let boardId = routeParams.boardId;\n    let inviterId = routeParams.inviter;\n    this.getInviteInfo(boardId, inviterId);\n  },\n  methods: {\n    async getInviteInfo(boardId, inviterId) {\n      var params = {\n        boardId: boardId,\n        inviterId: inviterId\n      };\n      var fullURL = BASE_URL + \"board/getInviteInfo\";\n      await axios.post(fullURL, params).then(response => {\n        console.log(\"invitation response: \", response.data);\n        if (response.data != null) {\n          let data = response.data;\n          if (data.statusCode == 200) {\n            let resp = data.resp;\n            this.inviter = resp.inviter;\n            this.board = resp.board;\n            if (this.board == null || this.inviter == null) {\n              alert(\"This invitation link is invalid, please ask the inviter to send you a valid invitation link.\");\n            }\n          } else {\n            alert(data.msg);\n          }\n        }\n      });\n    },\n    handleJoinBoard() {\n      console.log(\"join board\");\n      // sign up the user on an overlay UI, \n      // add his id to the board's membership\n      // navigate to all boards page\n      // change fetch all boards api logic to fetch data based on members\n    }\n  }\n};","map":{"version":3,"names":["ref","BASE_URL","axios","LoginView","components","setup","inviter","board","isAuth","mounted","routeParams","$route","params","console","log","boardId","inviterId","getInviteInfo","methods","fullURL","pos